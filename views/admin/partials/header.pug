header.header 
  .inner-logo 
    a(href=`/${pathAdmin}/dashboard`)
      span 28
      span Admin 
  .inner-wrap 
    .inner-notify 
      img(src="/admin/assets/images/icon-bell.svg")
      span 6
    .inner-account 
      .inner-avatar 
        img(src=account.avatar)
      .inner-text 
        .inner-name #{account.fullName}
        .inner-role #{account.roleName}
    button.inner-menu-mobile 
      <i class="fa-solid fa-bars"></i>

